<div id="<%= dom_id event %>">
  <p>
    <strong>Title:</strong>
    <%= event.title %>
  </p>

  <p>
    <strong>Start time:</strong>
    <%= event.start_time %>
  </p>

  <p>
    <strong>End time:</strong>
    <%= event.end_time %>
  </p>

  <p>
    <strong>Aircraft:</strong>
    <%= event.aircraft_id %>
  </p>

  <p>
    <strong>Instructor:</strong>
    <%= event.instructor_user_id %>
  </p>
  
  <p>
    <strong>User:</strong>
    <%= event.user_id %>
  </p>

</div>
